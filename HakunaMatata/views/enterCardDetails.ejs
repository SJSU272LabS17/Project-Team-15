<div id="carousel-example-generic" class="carousel intro slide">
	<!-- Indicators -->
	<!-- Wrapper for slides -->
	<!-- First slide -->
	<div class="item active" style="background-image:url(https://s3-eu-west-1.amazonaws.com/layanademo/s1-min.jpg)">
		<h1 data-animation="animated bounceInUp">Hakuna Matata </h1>
		<div class="carousel-caption">
			<div class="signupHM">
				<form role = "form" name="signupform" ng-submit="submit()">

					<!-- First Name -->
					<div class="form-group" align="left">
						<label for="firstname">Full Name</label>
						<input type="text" class="form-control" ng-model="firstname" placeholder="Enter Full Name" required/>
						<!-- <label ng-show = "signupform.firstname.$error.required">First Name is Required</label> -->
					</div>

					<!-- Email -->
					<div class="formm-group" align="left">
						<label for="cardtype">Card Type</label>
						<ol>
							<li>
								<input id=visa name=cardtype type=radio />
								<label for=visa>VISA</label>
							</li>
							<li>
								<input id=amex name=cardtype type=radio />
								<label for=amex>AmEx</label>
							</li>
							<li>
								<input id=mastercard name=cardtype type=radio />
								<label for=mastercard>Mastercard</label>
							</li>
						</ol>
					</div>
					<div class="form-group" align="left">
						<label for="text">CardNumber</label>
						<input type="text" class="form-control" ng-model="email" placeholder="Enter Card Number" required/>
					</div>

					<!-- Password -->
					<div class="form-group">
						<table>
							<th> <label for="birthday">Date of Expiry</label></th>
							<th></th>
							<th style="width: 500px;"> <label for="contact">CVV</label></th>

							<tr>
								<td> <!--<script>
                                            webshims.setOptions('forms-ext', {types: 'date'});
                                            webshims.polyfill('forms forms-ext');
                                        </script> -->
									<input type="date" class="form-control" ng-model="dateOfBirth" required ></input></td>
								<td>   </td>
								<td>
									<input type="text" class="form-control" ng-model="contact" placeholder="Enter Contact" required/>
								</td>
							</tr>
						</table>
					</div>

					<!-- Location -->
					<div class="form-group" align="left">

						<label for="lastname">Address</label>
						<input type="text" class="form-control" ng-model="location" placeholder="Enter Location" required/>
					</div>

					<table>
						<tr>
							<td style="width: 500px;"><a href="thankyou">Buy It</a></td>
							<!-- <td style="width: 500px;"><button  class="btn btn-info btn-block" href="/myHome/#events">Cancel</button></td> -->
						</tr>
					</table>

				</form>
				<a href="myHome/#events">Cancel</a>
			</div>
