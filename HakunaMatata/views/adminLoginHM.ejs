<!-- Section Intro Slider
================================================== -->
<div id="carousel-example-generic" class="carousel intro slide">
	<!-- Indicators -->
	<!-- Wrapper for slides -->
	<!-- First slide -->
	<div class="item active" style="background-image:url(https://s3-eu-west-1.amazonaws.com/layanademo/s1-min.jpg)">
		<h1 data-animation="animated bounceInUp">
			Hakuna Matata </h1>
		<h2>Welcome to Admin Login</h2>
		<div class="carousel-caption">
			<div class="adminLoginHM">
			<form role="form">
				<!--<div class="alert alert-danger">
                    <a class="close" data-dismiss="alert" href="#">Ã—</a>Incorrect Username or Password!
                </div>-->
				<div class="form-group" align="left">
					<label for="email">Email</label>
					<input id="login-email" type="text" class="form-control" name="username" value="" placeholder="email" ng-model="email" required>
				</div>

				<div class="form-group" align="left">
					<label for="password">Password</label>
					<input id="login-password" type="password" class="form-control" name="password" placeholder="password" ng-model="password" required>
				</div>

				<div class="input-group">
					<div class="checkbox" style="margin-top: 0px;">
						<label>
							<input id="login-remember" type="checkbox" name="remember" value="1"> Remember me
						</label>
					</div>
				</div>

				<button type="submit" class="btn btn-success" ng-click="submit()">Sign in</button>

				<hr style="margin-top:10px;margin-bottom:10px;" >
			</form>
		</div>
		</div>
	</div>

</div>
