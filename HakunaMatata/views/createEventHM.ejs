<div id="carousel-example-generic" class="carousel intro slide">
    <!-- Indicators -->
    <!-- Wrapper for slides -->
    <!-- First slide -->
    <div class="item active" style="background-image:url(https://s3-eu-west-1.amazonaws.com/layanademo/s1-min.jpg)">
        <h1 data-animation="animated bounceInUp">
            Hakuna Matata </h1>
        <div class="carousel-caption">
            <div class="createEventHM">
            <form role = "form" name="signupform" ng-submit="createEvent()">

                <!-- First Name -->
                <div class="form-group" align="left">
                    <label for="eventName">Event Name</label>
                    <input type="text" class="form-control" ng-model="eventName" placeholder="Enter Event Name" required/>
                </div>

                <!-- Email -->
                <div class="form-group" align="left">
                    <label for="description">Description</label>
                    <input type="text" class="form-control" ng-model="description" placeholder="Enter description" required/>
                </div>

                <div class="form-group" align="left">
                    <label for="eventType">Event Type</label>
                    <input type="text" class="form-control" ng-model="eventType" placeholder="Enter event type" required/>
                </div>

                <!--  -->
                <div class="form-group" align="left">
                    <label for="Price">Price</label>
                    <input type="text" class="form-control" ng-model="price" placeholder="Enter Price" required/>
                </div>


                <div class="form-group" align="left">
                    <label for="contact">Quantity</label>
                    <input type="text" class="form-control" ng-model="quantity" placeholder="quantity" required/>
                </div>

                <div class="form-group" align="left">

                    <label for="birthday">Date of Event</label>
                    <script>
                        webshims.setOptions('forms-ext', {types: 'date'});
                        webshims.polyfill('forms forms-ext');
                    </script>
                    <div >
                        <input type="text" class="form-control" ng-model="dateOfEvent" required ></input>
                    </div>
                </div>

                <!-- Submit Button -->
                <button type="submit" class="btn btn-success btn-block" >Create Event</button>
                <!-- Submit Button -->
               <!--  <button  class="btn btn-info btn-block" ng-click="backToSignin()">Back to Home</button> -->
            </form>
            </div>
        </div>
    </div>
