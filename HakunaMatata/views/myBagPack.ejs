<div ng-controller="myBagPack">

	<!--<a class="btn btn-success" ng-click= "signout()">sign Out</a>-->
	<!-- <div  style="background-image:url(/images/geekBackground.jpg)"> -->
		<!-- <img class="card-img-top img-fluid" src="/images/geekBackground.jpg" alt="Free Bootstrap Portfolio Template"> -->
	<div class="container">


		<div class="well well-sm" data-animation="animated fadeInLeft">
			<h2><strong>You are going to!</strong></h2>
		</div>

		<div id="products" class="row list-group">
			<div class="item  col-xs-4 col-lg-4"  ng-repeat="event in events">
				<img class="group list-group-image" src="images/{{event.images}}" height="300" width="350" alt="" />
				<div data-animation="animated fadeInLeft">
					<!--<img class="group list-group-image" src="http://placehold.it/400x250/000/fff" alt="" />-->

					<div class="caption">
						<h3 class="group inner list-group-item-heading">
							<b>{{event.EventName}}</b></h3>
						<p class="group inner list-group-item-text">
						<h4><i>{{event.EventDescription}}</i></h4></p>
						</br>
						<div class="row">
							<div class="col-xs-12 col-md-6">
								<p><h4><b><i>Tickets Left:</i> {{event.Qty}}</b></h4>
						        </p>
								<p class="lead">
									<b><i>Its just:</i>  ${{event.Price}}</b></p>
							</div>
							<!--<div class="col-xs-12 col-md-6">
								<a class="btn btn-success" ng-click= "AddToCart(event.EventId)"><i>Buy Tickets</i></a>
								<!-- <a href="#" class="btn btn-ghost btn-lg">Buy Tickets!</a>
							</div> -->
							<a class="btn btn-success" href="/enterCardDetails"><i>Buy It</i></a>
						</div>
						</br>
					</div>
				</div>
			</div>
		</div>
	</div>
	</div>
</div>
