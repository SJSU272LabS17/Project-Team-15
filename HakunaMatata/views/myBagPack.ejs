<body ng-app="myBagPack" >


<nav class="navbar navbar-default navbar-fixed-top">
	<div class="container">
		<div class="navbar-header">
			<a class="navbar-brand" href="/"><img src="images/HM1.png" /></a>
		</div>

		<ul class="nav navbar-nav navbar-right">
			<!-- 	<li><a href="../userProfile#/"><i class="fa fa-user"></i> User Profile</a></li> -->
			<!-- <li><a href="../userProfile#/cart"><i class="fa fa-shopping-cart"></i> Cart</a></li> -->
